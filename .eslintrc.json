{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "plugin:react/jsx-runtime", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-one-expression-per-line": 0,
    "react/react-in-jsx-scope": 0,
    "max-len": ["error", { "ignoreComments": true, "ignoreStrings": true }]
  }
}
